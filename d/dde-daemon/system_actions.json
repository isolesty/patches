{
	"Actions": [
	{
		"Key": "launcher",
		"Action": "dde-launcher"
	},
	{
		"Key": "terminal",
		"Action": "/usr/lib/deepin-daemon/default-terminal"
	},
	{
		"Key": "lock-screen",
		"Action": "dde-lock"
	},
	{
		"Key": "show-dock",
		"Action": "dbus-send --type=method_call --dest=com.deepin.daemon.Dock /dde/dock/HideStateManager dde.dock.HideStateManager.ToggleShow"
	},
	{
		"Key": "logout",
		"Action": "dde-shutdown"
	},
	{
		"Key": "terminal-quake",
		"Action": "deepin-terminal --quake-mode"
	},
	{
		"Key": "screenshot",
		"Action": "gnome-screenshot -a -f ~/Desktop/$(date +'%F,%H-%M-%S').png"
	},
	{
		"Key": "screenshot-fullscreen",
		"Action": "gnome-screenshot -f ~/Desktop/$(date +'%F,%H-%M-%S').png"
	},
	{
		"Key": "screenshot-window",
		"Action": "gnome-screenshot -w -f ~/Desktop/$(date +'%F,%H-%M-%S').png"
	},
	{
		"Key": "screenshot-delayed",
		"Action": "gnome-screenshot -d 5 -f ~/Desktop/$(date +'%F,%H-%M-%S').png"
	},
	{
		"Key": "file-manager",
		"Action": "gvfs-open ~"
	},
	{
		"Key": "disable-touchpad",
		"Action": "gsettings set com.deepin.dde.touchpad touchpad-enabled false"
	},
	{
		"Key": "wm-switcher",
		"Action": "dbus-send --type=method_call --dest=com.deepin.wm_switcher /com/deepin/wm_switcher com.deepin.wm_switcher.requestSwitchWM"
	}
	]
}
